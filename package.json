{
  "name": "reel-ai",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start -c",
    "dev:web": "expo start -c --web",
    "dev:android": "expo start -c --android",
    "android": "adb reverse tcp:54321 tcp:54321 && expo start -c --android",
    "ios": "expo start -c --ios",
    "web": "expo start -c --web",
    "clean": "rm -rf .expo node_modules",
    "postinstall": "npx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css",
    "db:status": "npx supabase status",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:reset": "npx supabase db reset",
    "db:push": "npx supabase db push",
    "db:types": "npx supabase gen types typescript --local > lib/database.types.ts",
    "db:new": "npx supabase migration new",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md,mdx}\" --cache"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5.0.6",
